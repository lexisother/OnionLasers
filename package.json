{
    "name": "onion-lasers-v13",
    "version": "2.0.0",
    "description": "A declarative, structure-agnostic, TypeScript-centric command handler for discord.js.",
    "main": "dist/index.js",
    "scripts": {
        "start": "tsc-watch --onSuccess \"npm run build\"",
        "build": "gulp && tsc",
        "test": "mocha --require ts-node/register --extension ts --recursive --unhandled-rejections=strict",
        "format": "prettier --write **/*",
        "prepare": "husky install",
        "prepack": "npm run build",
        "patch": "node patch.js"
    },
    "dependencies": {
        "discord.js": "^13.3.0",
        "glob": "^7.2.0"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.6",
        "@types/ws": "^8.2.0",
        "del": "^6.0.0",
        "dotenv": "^10.0.0",
        "gulp": "^4.0.2",
        "husky": "^7.0.4",
        "mocha": "^9.1.3",
        "prettier": "^2.4.1",
        "ts-node": "^10.4.0",
        "tsc-watch": "^4.5.0",
        "typescript": "^4.4.4"
    },
    "types": "typings/index.d.ts",
    "author": "WatDuhHekBro",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WatDuhHekBro/OnionLasers.git"
    },
    "bugs": {
        "url": "https://github.com/WatDuhHekBro/OnionLasers/issues"
    },
    "homepage": "https://github.com/WatDuhHekBro/OnionLasers#readme",
    "keywords": [
        "discord",
        "discord.js",
        "discord bot",
        "command handler",
        "typescript"
    ]
}
